@import '../style/index.less';

.systemsList {
  margin-top: 15px;
  overflow-y: auto;
  .listItem {
    padding: 5px 7px;
    margin-bottom: 5px;
    display: flex;
    align-items: center;
    .hover-background;
    > .logo {
      width: 40px;
      height: 40px;
      color: white;
      font-size: 30px;
      .center;
    }
    > .title {
      margin-left: 10px;
      overflow: hidden;
      text-overflow: ellipsis;
      white-space: nowrap;
    }
  }
}

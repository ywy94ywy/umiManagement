{
  "name": "cloud_front",
  "private": true,
  "version": "0.0.1",
  "description": "云平台前台",
  "license": "MIT",
  "scripts": {
    "start": "umi dev",
    "start:ana": "set ANALYZE=1 && umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.14",
    "antd": "^4.5.2",
    "antd-img-crop": "^3.9.0",
    "lint-staged": "^10.0.7",
    "js-cookie": "^2.2.1",
    "moment": "^2.27.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "umi": "^3.2.14",
    "yorkie": "^2.0.0"
  }
}
